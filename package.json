{
  "name": "mike-fowler",
  "private": true,
  "description": "Mike Fowlerâ€™s website",
  "version": "1.0.0",
  "author": "Mike Fowler <mike.fowler@airbnb.com>",
  "scripts": {
    "start": "concurrently -c inverse 'npm:start:*'",
    "start:gatsby": "gatsby develop",
    "start:apollo": "npm run generate-types",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "download-schema": "apollo schema:download --endpoint='http://localhost:8000/___graphql' schema.json",
    "generate-types": "apollo codegen:generate typings/__generated__ --outputFlat --schema=schema.json --target=typescript --queries='./src/**/*.{ts,tsx}' --tagName=gql",
    "generate-types:watch": "npm run generate-types -- --watch"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "@rebass/grid": "^6.0.0-3",
    "@types/styled-system": "^3.0.4",
    "emotion": "^9.2.8",
    "emotion-server": "^9.2.8",
    "emotion-theming": "^9.2.6",
    "gatsby": "^2.0.15",
    "gatsby-image": "^2.0.10",
    "gatsby-paginate": "^1.0.16",
    "gatsby-plugin-emotion": "^2.0.0-rc.1",
    "gatsby-plugin-extract-schema": "0.0.4",
    "gatsby-plugin-manifest": "^2.0.2-rc.1",
    "gatsby-plugin-offline": "^2.0.0-rc.4",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-typescript": "^2.0.0-rc.4",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^2.0.5",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-source-instagram": "^0.2.0",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.1",
    "luxon": "^1.4.4",
    "modular-scale": "^5.1.0",
    "palx": "^1.0.3",
    "polished": "^2.1.1",
    "prism-solarized-dark": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.8",
    "react-helmet": "^5.2.0",
    "rebass": "^3.0.0-1",
    "styled-system": "^3.0.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "@types/luxon": "^1.4.0",
    "@types/node": "^10.9.4",
    "@types/reach__router": "^1.0.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "apollo": "^1.9.2",
    "concurrently": "^4.1.0",
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-graphql": "0.0.6",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikefowler/website"
  }
