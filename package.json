    "@types/node": "^10.12.15",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.7",
    "apollo": "^2.1.8",
    "concurrently": "^4.1.0",
    "prettier": "^1.15.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-graphql": "0.0.6",
