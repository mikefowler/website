{
  "name": "mike-fowler",
  "private": true,
  "description": "Mike Fowlerâ€™s website",
  "version": "1.0.0",
  "author": "Mike Fowler <mike.fowler@airbnb.com>",
  "scripts": {
    "start": "concurrently -c inverse 'npm:start:*'",
    "start:gatsby": "gatsby develop",
    "start:apollo": "npm run apollo:generate-types:watch",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "apollo:download-schema": "apollo service:download --endpoint='http://localhost:8000/___graphql' schema.json",
    "apollo:generate-types": "apollo client:codegen typings/__generated__ --outputFlat --localSchemaFile=schema.json --target=typescript --includes='./src/**/*.{ts,tsx}' --tagName=gql",
    "apollo:generate-types:watch": "npm run generate-types -- --watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@reach/router": "^1.2.1",
    "@rebass/emotion": "^3.0.0",
    "@rebass/grid": "^6.0.0-7",
    "@types/styled-system": "^3.0.9",
    "emotion": "^10.0.5",
    "emotion-server": "^10.0.5",
    "emotion-theming": "^10.0.5",
    "gatsby": "^2.0.70",
    "gatsby-image": "^2.0.25",
    "gatsby-paginate": "^1.0.16",
    "gatsby-plugin-collections": "^0.1.1",
    "gatsby-plugin-emotion": "^3.0.1",
    "gatsby-plugin-extract-schema": "0.0.4",
    "gatsby-plugin-manifest": "^2.0.12",
    "gatsby-plugin-offline": "^2.0.20",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-prismjs": "^3.1.4",
    "gatsby-remark-responsive-iframe": "^2.0.7",
    "gatsby-remark-smartypants": "^2.0.7",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-source-instagram": "^0.2.0",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "luxon": "^1.8.2",
    "modular-scale": "^5.1.0",
    "palx": "^1.0.3",
    "polished": "^2.3.1",
    "prism-solarized-dark": "^1.0.1",
